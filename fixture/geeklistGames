															<boardgamefamily objectid="20789">Jass</boardgamefamily>
															<boardgameversion objectid="70123">Public Domain version</boardgameversion>
																						<boardgamemechanic objectid="2004">Set Collection</boardgamemechanic>
															<boardgamefamily objectid="98">Traditional Card Games</boardgamefamily>
															<boardgamemechanic objectid="2009">Trick-taking</boardgamemechanic>
						
