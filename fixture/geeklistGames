															<boardgamepublisher objectid="3320">(Unknown)</boardgamepublisher>
															<boardgamesubdomain objectid="4666">Abstract Games</boardgamesubdomain>
															<boardgameversion objectid="137138">Acme Ruler Canadian red edition</boardgameversion>
															<boardgamepublisher objectid="14618">Acme Ruler Company</boardgamepublisher>
															<boardgameversion objectid="132583">Acme Ruler Two Lane Board Edition</boardgameversion>
															<boardgameversion objectid="132590">Are-Jay  Big D Edition</boardgameversion>
															<boardgameversion objectid="195234">Are-Jay edition</boardgameversion>
															<boardgameversion objectid="137145">Are-Jay Folding edition</boardgameversion>
															<boardgamepublisher objectid="4074">Are-Jay Game Company</boardgamepublisher>
															<boardgamepublisher objectid="16355">Bello Games New York, Inc.</boardgamepublisher>
