																						<boardgamedesigner objectid="9393">Néstor Romeral Andrés</boardgamedesigner>
															<boardgameartist objectid="9393">Néstor Romeral Andrés</boardgameartist>
																						<boardgamemechanic objectid="2002">Tile Placement</boardgamemechanic>
						
